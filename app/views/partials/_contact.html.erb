<section id="contact">
	<div class="container">
		<h1>Contact</h1>
	
		<form novalidate>
			<label for="name">Name</label> <br>
			<input id="name"><br>

			<label for="email">Email</label><br>
			<input type="email" id="email"><br>

			<label for="message">Message</label><br>
			<textarea id="message"></textarea><br>

			<input type="submit" value="Send">
		</form>
	</div>
</section>